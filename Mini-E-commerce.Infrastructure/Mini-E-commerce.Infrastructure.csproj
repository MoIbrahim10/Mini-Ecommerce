    </PackageReference>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="7.0.7" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Mini-E-commerce.Application\Mini-E-commerce.Application.csproj" />
    <ProjectReference Include="..\Models\Mini-E-commerce.Domain.csproj" />
  </ItemGroup>

</Project>
